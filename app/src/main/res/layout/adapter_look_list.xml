<?xml version="1.0" encoding="utf-8"?>
<LinearLayout xmlns:android="http://schemas.android.com/apk/res/android"
    android:id="@+id/ll_view"
    android:layout_width="match_parent"
    android:layout_height="wrap_content"
    android:background="@color/white"
    android:orientation="vertical">

    <RelativeLayout
        android:id="@+id/rl_newst_order"
        style="@style/match_wrap"
        android:background="@drawable/personitem"
        android:paddingRight="@dimen/margin_10"
        android:paddingLeft="@dimen/margin_10"
        android:paddingTop="@dimen/margin_10"
        >

        <ImageView
            android:id="@+id/iv_01"
            style="@style/wrap_content"
            android:layout_centerVertical="true"
            android:background="@drawable/null_car_lin" />

        <ImageView
            android:id="@+id/iv_02"
            style="@style/wrap_content"
            android:layout_alignParentRight="true"
            android:layout_centerVertical="true"
            android:background="@drawable/look_car" />

        <LinearLayout
            style="@style/match_wrap"
            android:layout_alignParentTop="true"
            android:layout_toLeftOf="@id/iv_02"
            android:layout_toRightOf="@+id/iv_01"
            android:orientation="vertical"
            android:paddingLeft="10dp"
            android:paddingRight="10dp">

            <TextView
                android:id="@+id/tv_createAddress"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:textSize="15sp" />

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="4dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    style="@style/wrap_content"
                    android:layout_centerVertical="true"
                    android:background="@drawable/time" />

                <TextView
                    android:id="@+id/tv_createDate"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:textColor="@color/text_gray"
                    android:textSize="13sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">

                <ImageView
                    style="@style/wrap_content"
                    android:layout_centerVertical="true"
                    android:background="@drawable/car" />

                <TextView
                    android:id="@+id/tv_owner_vehicle"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:layout_marginLeft="4dp"
                    android:text="箱式"
                    android:textColor="@color/text_gray"
                    android:textSize="13sp" />

            </LinearLayout>

            <LinearLayout
                android:layout_width="match_parent"
                android:layout_height="wrap_content"
                android:layout_marginTop="3dp"
                android:gravity="center_vertical"
                android:orientation="horizontal">


                <TextView
                    android:id="@+id/tv_destination"
                    android:layout_width="wrap_content"
                    android:layout_height="wrap_content"
                    android:textSize="15sp" />

            </LinearLayout>


        </LinearLayout>
    </RelativeLayout>


    <RelativeLayout
        android:id="@+id/rl_account_type"
        android:layout_width="match_parent"
        android:layout_height="wrap_content">

        <LinearLayout
            android:id="@+id/ll_01"
            android:layout_width="wrap_content"
            android:layout_height="wrap_content"
            android:gravity="center_vertical"
            android:orientation="horizontal"
            android:padding="10dp">

            <ImageView
                style="@style/wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginLeft="4dp"
                android:background="@drawable/shilian" />

            <TextView
                android:id="@+id/tv_margin_isnull"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginLeft="4dp"
                android:gravity="center_vertical"
                android:text="暂无"
                android:textColor="@color/text_gray"
                android:textSize="14sp" />

        </LinearLayout>

        <LinearLayout
            android:layout_width="match_parent"
            android:layout_height="wrap_content"
            android:layout_alignParentRight="true"
            android:layout_toRightOf="@id/ll_01"
            android:gravity="right|center_vertical"
            android:orientation="horizontal"
            android:padding="10dp">

            <ImageView
                style="@style/wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="4dp"
                android:background="@drawable/pin" />

            <TextView
                android:id="@+id/tv_distanceAround"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="4dp"
                android:textColor="@color/text_gray"
                android:textSize="14sp" />

            <ImageView
                style="@style/wrap_content"
                android:layout_centerVertical="true"
                android:layout_marginRight="4dp"
                android:background="@drawable/clock" />

            <TextView
                android:id="@+id/tv_shelvesDate"
                android:layout_width="wrap_content"
                android:layout_height="wrap_content"
                android:layout_marginRight="4dp"
                android:textColor="@color/text_gray"
                android:textSize="14sp" />

        </LinearLayout>
    </RelativeLayout>

    <View style="@style/lin">

    </View>

    <View
        style="@style/lin"
        android:layout_marginTop="10dp">

    </View>

</LinearLayout>